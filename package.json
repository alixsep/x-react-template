{
  "name": "x-react-template",
  "description": "The unknown React.JS template ðŸ”®",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --open",
    "build": "vite build",
    "prepare": "husky",
    "preview": "vite preview",
    "preview:test": "start-server-and-test preview 5000",
    "test": "vitest",
    "test:ci": "vitest run",
    "test:e2e": "pnpm preview:test 'cypress open'",
    "test:e2e:ci": "vite build && pnpm preview:test 'cypress run --record'",
    "test:e2e:headless": "pnpm preview:test 'cypress run'",
    "format": "prettier -uw --cache --ignore-path .gitignore **/*.{ts,tsx,js,jsx,json,md,mdx,html}",
    "run-tsc": "tsc",
    "run-eslint": "eslint --cache --fix --ignore-path .gitignore **/*.{ts,tsx,js,jsx}",
    "run-stylelint": "stylelint --aei --cache --fix --ignore-path .gitignore **/*.{css,scss}",
    "lint": "run-p run-*",
    "validate": "vite build && run-p lint test:ci test:e2e:headless",
    "generate-pwa-assets": "pwa-assets-generator --preset minimal-2023 public/favicon.svg",
    "commit": "cz"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.2.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-redux": "^9.1.1",
    "react-router-dom": "^6.23.0",
    "the-new-css-reset": "^1.11.2"
  },
  "devDependencies": {
    "@testing-library/cypress": "^10.0.1",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^15.0.5",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^20.12.7",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "@vite-pwa/assets-generator": "^0.2.4",
    "@vitejs/plugin-react-swc": "^3.6.0",
    "commitizen": "^4.3.0",
    "cypress": "^13.8.1",
    "cypress-vite": "^1.5.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.6",
    "husky": "^9.0.11",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.2.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.5",
    "sass": "^1.75.0",
    "start-server-and-test": "^2.0.3",
    "stylelint": "^16.4.0",
    "stylelint-config-standard-scss": "^13.1.0",
    "stylelint-prettier": "^5.0.0",
    "typescript": "^5.4.5",
    "vite": "^5.2.10",
    "vite-plugin-pwa": "^0.19.8",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.5.2"
  }
}
