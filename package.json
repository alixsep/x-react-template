{
  "name": "x-react-template",
  "description": "The unknown React.JS template ðŸ”®",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --open",
    "build": "vite build",
    "prepare": "husky install",
    "preview": "vite preview",
    "preview:test": "start-server-and-test preview 5000",
    "test": "vitest",
    "test:ci": "vitest run",
    "test:e2e": "pnpm preview:test 'cypress open'",
    "test:e2e:ci": "vite build && pnpm preview:test 'cypress run --record'",
    "test:e2e:headless": "pnpm preview:test 'cypress run'",
    "format": "prettier -uw --cache --ignore-path .gitignore **/*.{ts,tsx,js,jsx,json,md,mdx,html}",
    "run-tsc": "tsc",
    "run-eslint": "eslint --cache --fix --ignore-path .gitignore **/*.{ts,tsx,js,jsx}",
    "run-stylelint": "stylelint --aei --cache --fix --ignore-path .gitignore **/*.{css,scss}",
    "lint": "run-p run-*",
    "validate": "run-p lint test:ci test:e2e:headless",
    "generate-pwa-assets": "pwa-assets-generator --preset minimal-2023 public/favicon.svg",
    "commit": "cz"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.0.4",
    "react-router-dom": "^6.21.1",
    "the-new-css-reset": "^1.11.2"
  },
  "devDependencies": {
    "@testing-library/cypress": "^10.0.1",
    "@testing-library/jest-dom": "^6.2.0",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^20.10.6",
    "@types/react": "^18.2.46",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "@vite-pwa/assets-generator": "^0.2.2",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "commitizen": "^4.3.0",
    "cypress": "^13.6.2",
    "cypress-vite": "^1.5.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "husky": "^8.0.3",
    "jsdom": "^23.0.1",
    "lint-staged": "^15.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.1.1",
    "sass": "^1.69.7",
    "start-server-and-test": "^2.0.3",
    "stylelint": "^16.1.0",
    "stylelint-config-standard-scss": "^13.0.0",
    "stylelint-prettier": "^5.0.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "vite-plugin-pwa": "^0.17.4",
    "vite-tsconfig-paths": "^4.2.3",
    "vitest": "^1.1.1"
  }
}
